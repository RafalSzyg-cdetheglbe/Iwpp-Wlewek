        <Button x:Name="licz" Content="Oblicz" HorizontalAlignment="Left" Height="22" Margin="136,526,0,0" VerticalAlignment="Top" Width="116" Click="licz_Click"/>
        <TextBox x:Name="predkosc_wlewania" HorizontalAlignment="Left" Margin="194,492,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Prędkość welwania" HorizontalAlignment="Left" Height="29" Margin="56,492,0,0" VerticalAlignment="Top" Width="130"/>

    </Grid>
</Window>
