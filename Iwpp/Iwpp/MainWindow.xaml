        <TextBox x:Name="przeplyw_obj_m_s" Grid.Column="1" HorizontalAlignment="Left" Margin="260,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18"/>
        <TextBox x:Name="przeplyw_obj_l_m" Grid.Column="1" HorizontalAlignment="Left" Margin="260,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18" />
        <TextBox x:Name="przeplyw_obj_m_min" Grid.Column="1" HorizontalAlignment="Left" Margin="260,289,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="18"/>
