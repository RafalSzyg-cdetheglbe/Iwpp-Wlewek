        <Label Grid.Column="1" Content="Skala" HorizontalAlignment="Left" Margin="157,60,0,0" VerticalAlignment="Top"/>
        <Label Grid.Column="1" Content="skala przepływu&#xD;&#xA;obiętościowego" HorizontalAlignment="Left" Margin="120,120,0,0" VerticalAlignment="Top" Width="97" Height="43"/>
        <Label Grid.Column="1" Content="przepływ obiętościowy [m/s]" HorizontalAlignment="Left" Margin="99,175,0,0" VerticalAlignment="Top"/>
        <Label Grid.Column="1" Content="przepływ obiętościowy [l/min]" HorizontalAlignment="Left" Margin="91,226,0,0" VerticalAlignment="Top"/>
        <Label Grid.Column="1" Content="przepływ obiętościowy [m/min]" HorizontalAlignment="Left" Margin="76,285,0,0" VerticalAlignment="Top"/>
        <Label Grid.Column="1" Content="przepływ w jednym wylewie [m/s]" HorizontalAlignment="Left" Margin="76,350,0,0" VerticalAlignment="Top"/>
